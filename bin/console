#!/usr/bin/env ruby
# frozen_string_literal: true

require "bundler/setup"
require "cyprus_postal_codes"

@client = CyprusPostalCodes::Client.new(api_key: ENV["CYPRUS_POSTAL_CODES_API_KEY"])

puts
puts "We've created a 'client' instance variable as a CyprusPostalCodes::Client using ENV['CYPRUS_POSTAL_CODES_API_KEY'] as the API key."
puts

require "irb"
IRB.start(__FILE__)
